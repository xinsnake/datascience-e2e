{
  "name": "Data Science E2E",
  "dockerComposeFile": "docker-compose.yml",
  "service": "workspace",
  "context": "..",
  "workspaceFolder": "/workspace",
  "settings": {
    "editor.formatOnSave": true,
    "python.formatting.autopep8Path": "/opt/conda/bin/autopep8",
    "python.formatting.blackPath": "/opt/conda/bin/black",
    "python.formatting.yapfPath": "/opt/conda/bin/yapf",
    "python.linting.banditPath": "/opt/conda/bin/bandit",
    "python.linting.enabled": true,
    "python.linting.flake8Path": "/opt/conda/bin/flake8",
    "python.linting.mypyPath": "/opt/conda/bin/mypy",
    "python.linting.pycodestylePath": "/opt/conda/bin/pycodestyle",
    "python.linting.pydocstylePath": "/opt/conda/bin/pydocstyle",
    "python.linting.pylintEnabled": true,
    "python.linting.pylintPath": "/opt/conda/bin/pylint",
    "terminal.integrated.shell.linux": "/bin/bash",
  },
  "extensions": [
    "ms-python.python",
    "paiqo.databricks-vscode",
    "yzhang.markdown-all-in-one"
  ],
  "postAttachCommand": "mkdir -p /root/.ssh && touch /root/.ssh/config",
}